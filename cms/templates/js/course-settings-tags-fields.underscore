<ul class='list-input tag-list'>
    <% for (var i = 0; i < courseTags.length; i++) { %>
        <li>
          <button class='tag-item' data-index='<%- i %>'>
            <span class='tag-name'><%- courseTags[i] %></span>
            <span class='fa fa-times'></span>
          </button>
        </li>
    <% } %>
</ul>